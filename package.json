{
  "name": "schilling-new",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "generate-types": "swagger-typescript-api -p https://url -o ./src/types --axios",
    "generate-local-types": "swagger-typescript-api -p http://url -o ./src/types --axios",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w -u .",
    "pre-commit": "npx lint-staged",
    "lint": "prettier -w -u . && npx eslint . --ext ts,tsx --fix",
    "prepare": "husky",
    "husky": "npx husky init && echo \"pnpm run pre-commit\" > .husky/pre-commit  && echo \"npx --no -- commitlint --edit ${1}\" > .husky/pre-commit-msg",
    "createFakeApi": "node ./jServer/fakeApis/index.js",
    "useFakeApi": "json-server ./jServer/fakeApis/Api.json --port 3001",
    "fakeApi": "npm run createFakeApi && npm run useFakeApi"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@mdi/font": "^7.4.47",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@tanstack/react-query": "^5.50.1",
    "@tanstack/react-query-devtools": "^5.51.9",
    "@tanstack/react-router": "^1.43.15",
    "@tanstack/react-table": "^8.19.2",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "lucide-react": "^0.407.0",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@tanstack/eslint-plugin-query": "^5.50.1",
    "@tanstack/router-devtools": "^1.43.15",
    "@tanstack/router-plugin": "^1.43.13",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-tailwindcss": "3.13.1",
    "husky": "^9.0.11",
    "json-server": "^1.0.0-beta.1",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "swagger-typescript-api": "^13.0.5",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{ts,tsx,jsx}": [
      "npx eslint . --ext ts,tsx --fix",
      "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    ]
  },
  "engines": {
    "node": "20.x"
  }
}
